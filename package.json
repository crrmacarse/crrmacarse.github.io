{
  "name": "portfolio",
  "homepage": "https://crrmacarse.github.io/",
  "version": "1.0.17",
  "private": true,
  "scripts": {
    "hot": "webpack-dev-server --open --hot --profile --progress --colors --config=webpack.config.babel.js --env=hot",
    "dev": "webpack --hot --profile --progress --config=webpack.config.babel.js --env=development",
    "prod": "webpack --profile --progress --config=webpack.config.babel.js --env=production",
    "analyze": "source-map-explorer './dist/*.js'",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-pattern node_modules/",
    "test": "jest --coverage",
    "test-watch": "jest --verbose --watch",
    "test-update": "jest --updateSnapshot",
    "pre-deploy": "npm run test && npm run prod",
    "deploy": "npm run pre-deploy && npm version patch && cp dist/index.html dist/404.html && gh-pages -b master -d dist",
    "deploy:windows": "npm run pre-deploy && npm version patch && copy 'dist/index.html' 'dist/404.html' && gh-pages -b master -d dist"
  },
