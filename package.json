{
  "name": "portfolio",
  "homepage": "https://crrmacarse.github.io/",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "hot": "webpack-dev-server --open --hot --profile --progress --colors --config=webpack.config.babel.js --env=hot",
    "dev": "webpack --hot --profile --progress --config=webpack.config.babel.js --env=development",
    "prod": "webpack --profile --progress --config=webpack.config.babel.js --env=production",
    "build:stats": "webpack --profile --json --config=webpack.config.babel.js --env=production > compilation-stats.json",
    "analyze": "webpack-bundle-analyzer compilation-stats.json dist",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-pattern node_modules/",
    "lint:prod": "eslint --ext .js,.ts,.tsx src --ignore-pattern src/**/*.test.tsx",
    "test": "jest --verbose --colors --coverage",
    "test:watch": "jest --colors --watch",
    "test-update": "jest --updateSnapshot",
    "sitemap": "npx babel-node sitemap-builder.js",
    "copy": "cp essentials/* dist -r",
    "copy:windows": "copy essentials/* dist",
    "pre-deploy": "npm run test && npm run prod && npm run sitemap",
    "serve:prod":"serve dist",
    "deploy": "npm run pre-deploy && npm run copy && gh-pages -b master -d dist",
    "deploy:windows": "npm run pre-deploy && npm run copy:windows && gh-pages -b master -d dist"
  },
